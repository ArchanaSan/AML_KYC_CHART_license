
<!DOCTYPE HTML>
<html>
<head>
  <!--<script src="https://canvasjs.com/assets/script/canvasjs.min.js"></script>-->
<center><h2>{{#if users}}
<script>
window.onload = function () {

var chart = new CanvasJS.Chart("chartContainer", {
exportEnabled: true,
animationEnabled: true,
theme: "light1", // "light1", "light2", "dark1", "dark2"
title:{
text: "Average Volume Transactions Yesterday in Range"
},
axisY: {
title: "Total Customers"
    //interval: 100
},
  dataPointMaxWidth : 50,

data: [{
type: "column",
showInLegend: true,
legendMarkerColor: "grey",
legendText: "Average Volume Trasactions Yesterday in Range",
    /*showInLegend: true,
    legendText: "Gold",
       color: "pink",*/

    dataPoints: [
      {{#each users}}
      { y: {{count}},  label: "{{TransactionCountRenge}}" },
      {{/each}}
]

}]
});
chart.render();

}
</script>

{{else}}
  There is no related data present.
{{/if}}</center></h2>


</head>
<body>
<div id="chartContainer" style="height: 300px; width: 100%; "></div><br><br><br><br><br>

